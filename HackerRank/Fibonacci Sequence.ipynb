    "memory = {}\n",
    "def fibonacci(n):\n",
    "    if n < 2:\n",
    "        return n\n",
    "    if not n in memory.keys():\n",
    "        memory[n] = fibonacci(n-1) + fibonacci(n-2)\n",
    "    return memory[n]\n",
    "print(fibonacci(7))"
   ]
